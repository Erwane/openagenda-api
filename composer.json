{
    "name": "openagenda/api-sdk",
    "description": "OpenAgenda SDK for PHP",
    "type": "library",
    "keywords": [
        "openagenda",
        "api"
    ],
    "homepage": "https://openagenda.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Erwane Breton",
            "homepage": "https://erwane-breton.fr/",
            "email": "erwane@phea.fr"
        }
    ],
    "support": {
        "issues": "https://github.com/erwane/openagenda-api/issues",
        "source": "https://github.com/erwane/openagenda-api"
    },
    "require": {
        "php": "^7.1",
        "ext-json":  "*",
        "symfony/cache": "^4.0 | ^5.0",
        "psr/simple-cache": "^1.0 | ^2.0",
        "guzzlehttp/guzzle": "^6.0 | ^7.0",
        "ezyang/htmlpurifier": "^4.9",
        "league/html-to-markdown": "^4.0 | ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 | ^8.0 | ^9.0",
        "symfony/var-dumper": "^4.0 | ^5.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAgenda\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenAgenda\\Test\\": "tests/src"
        }
    }
}
