{
    "name": "erwane/openagenda-api",
    "description": "OpenAgenda SDK for PHP",
    "type": "library",
    "keywords": [
        "openagenda",
        "api"
    ],
    "homepage": "https://github.com/Erwane/openagenda-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Erwane Breton",
            "homepage": "https://erwane-breton.fr/",
            "email": "erwane@phea.fr"
        }
    ],
    "support": {
        "issues": "https://github.com/erwane/openagenda-api/issues",
        "source": "https://github.com/erwane/openagenda-api"
    },
    "require": {
        "php": "^7.2",
        "ext-json":  "*",
        "ezyang/htmlpurifier": "^4.9",
        "league/html-to-markdown": "^4.0 | ^5.0",
        "psr/http-client": ">=1.0",
        "psr/simple-cache": ">=1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "symfony/var-dumper": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAgenda\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenAgenda\\Test\\": "tests/src"
        }
    }
}
